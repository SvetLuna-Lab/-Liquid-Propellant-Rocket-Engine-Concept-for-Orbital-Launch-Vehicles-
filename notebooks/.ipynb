def est_isp(expansion_ratio, stage='first'):
    if stage == 'first':
        return 300 + 0.5 * (expansion_ratio - 10)
    else:
        return 420 + 0.6 * (expansion_ratio - 10)

def plot_isp_sweep(expansions):
    isp1 = [est_isp(e, 'first') for e in expansions]
    isp2 = [est_isp(e, 'upper') for e in expansions]
    plt.figure(figsize=(8,5))
    plt.plot(expansions, isp1, label='First stage (LOX+RP-1)')
    plt.plot(expansions, isp2, label='Upper stage (LOX+LHâ‚‚)')
    plt.axvline(cfg['ae_at'][0], color='grey', linestyle='--', label=f"Target Ae/At â‰ˆ {cfg['ae_at'][0]}")
    if len(cfg['ae_at']) > 1:
        plt.axvline(cfg['ae_at'][1], color='grey', linestyle='--', label=f"Target Ae/At â‰ˆ {cfg['ae_at'][1]}")
    plt.xlabel('Nozzle Expansion Ratio (Ae/At)')
    plt.ylabel('Estimated Isp (s)')
    plt.title('Estimated Isp vs Nozzle Expansion Ratio')
    plt.legend()
    plt.grid(True)
    plt.show()
